# In general, you can use the options available
# fastlane gym --help

# Remove the # in front of the line to enable the option

# scheme("Example")

# sdk("iphoneos9.0")

# App scheme name
scheme("MatchScoresTCA")
#provide provisioning profiles to use
export_options({
   method: "app-store",
   provisioningProfiles: { 
     "com.emre.MatchScoresTCA" => "match AppStore com.emre.MatchScoresTCA",
        }
})
# Specify the path to store .ipa file
output_directory("./fastlane/builds")
# Excludes bitcode from the build
include_bitcode(false)
# Excludes symbols from the build.
include_symbols(false)
